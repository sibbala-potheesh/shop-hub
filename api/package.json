{
  "name": "nestjs-ecommerce-sample",
  "version": "1.0.0",
  "description": "Sample NestJS e-commerce style app with Mongoose, Swagger, class-validator, Winston, and JWT auth",
  "main": "dist/main.js",
  "scripts": {
    "start": "node -r dotenv/config dist/main.js",
    "start:dev": "nest start --watch",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && mkdir -p dist/public && cp -r ../app/build/* public/",
    "seed": "ts-node -r tsconfig-paths/register -r dotenv/config src/seed.ts",
    "prestart:prod": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@vitest/coverage-v8": "^4.0.14",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.0",
    "mongoose": "^7.0.0",
    "nest-winston": "^1.10.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.38",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  }
}
